{
  "name": "hello3dmcp-server",
  "version": "0.2.1",
  "description": "MCP server for controlling 3D model visualization via Model Context Protocol",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "mcp:server": "node server.js",
    "build": "node esbuild.config.js && node build-mcpb.js",
    "build:bundle": "node esbuild.config.js",
    "build:package": "node build-mcpb.js",
    "start:prod": "node dist/hello3dmcp-server.js",
    "version:sync": "node scripts/sync-version.js",
    "version:patch": "node scripts/sync-version.js --patch",
    "version:minor": "node scripts/sync-version.js --minor",
    "version:major": "node scripts/sync-version.js --major",
    "version:set": "node scripts/sync-version.js",
    "preversion": "node scripts/sync-version.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "websocket",
    "3d",
    "server"
  ],
  "author": "Douglass Turner",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "ws": "^8.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "esbuild": "^0.27.2"
  }
}
